{
  "name": "ia-telegram-bot",
  "version": "1.0.0",
  "description": "Bot de Telegram con IA para procesamiento de comprobantes y facturas",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:clean": "tsc",
    "start": "node dist/index.js",
    "start:clean": "node dist/index.clean.js",
    "dev": "tsx watch src/index.clean.ts",
    "dev:clean": "tsx watch src/index.clean.ts",
    "dev:nodemon": "nodemon --exec tsx src/index.clean.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:coverage:ui": "vitest --ui --coverage"
  },
  "keywords": [
    "telegram",
    "bot",
    "ai",
    "invoice",
    "ocr",
    "vision"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "fs-extra": "^11.2.0",
    "openai": "^4.67.3",
    "pdf-poppler": "^0.2.3",
    "pdfjs-dist": "^3.11.174",
    "telegraf": "^4.16.3",
    "tesseract.js": "^6.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.9.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
